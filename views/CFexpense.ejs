<!DOCTYPE html>
<html lang="en">
    <%-include('./partials/head.ejs')%>
<body>
    <nav class="navbar bg-black">
      <div class="container-fluid">
        <span class="navbar-brand" onclick="history.back()" href="/" title="Back">
          <img 
          width="30"
          hegith="30"
          src="data:image/png;base64,">
         </span>
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item dropdown">
            <h5>
              <a
                class="nav-link breadcrumb-item active"
                aria-current="page"
                href="/employee/Onboard"
                role="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
                style="color: #fff !important"
              >
               Expense
              </a>
            </h5>
          </li>
        </ul>
      </div>
    </nav>
</body>
<!-- display section -->
            <!--bread crum div -->
            <div class="d-flex justify-content-between p-2 m-1 bg-success-subtle ">
                <h5 class="font-bold">Expense</h5>
                <%if(user.role === "Admin"){%>
                 <button class="btn btn-success" onclick="document.getElementById('myModal').style.display='block'">REQUEST EXPENSE</button>
                <%}%> 
               </div>
            <div class="h-59 p-1 d-flex flex-column" >
                 <%if(Expenses.length > 0){%>
                   <%Expenses.forEach(expense=>{%>
                     <span onclick="location.assign('')" class="list-group position-relative mb-2" title="Expense for <%=expense.Amount%>">
                       <div class="list-group-item list-group-item-action d-flex justify-content-between align-items-center" >
                           <small class="position-absolute top-0  start-50 translate-middle badge rounded-pill bg-warning"title='grand total of this bill'>
                             NGN<%=expense.Amount%>
                              <%if(expense.status==='Approved'){%>
                             <span class="badge bg-secondary">EXPENSED</span>
                             <%}%>
                           </small>
                               <p title="Manager">
                                <%WHID = WHID.filter(wh =>{%>
                                    <%return wh._id.toString() === expense.WHID.toString()%>
                                <%})%>
                                <%WHID.map(wh =>{%>
                                    <%=wh.WHName.toUpperCase()%>
                                <%})%>    
                               </p>
                             <div>
                               <p>Raised Date: <%=expense.Date%></p>
                             </div>
                             <div>
                               <p><%=expense.category%></p>
                             </div>
                             <p title="reference Number" >Exp No:<%=expense.refNo%></p>
                             <div>
                                <a href="" class="btn btn-outline">
                                    <img 
                                    width="20"
                                    height="20"
                                    src="data:image/png;base64,">
                                </a>
                                <a href="" class="btn btn-success">Approve </a>
                                <a href="" class="btn btn-danger">Reject</a>
                           </div> 
                              
                         </div> 
                         
                       </span>
                   <%})%>
                 <%}else{%>
                <p>Nothig to display</p>
                 <%}%>
             </div>
    </div>
</html>