<!DOCTYPE html>
<html lang="en">
  <%-include('./partials/head.ejs')%>
  <body>
    <nav class="navbar bg-black">
      <div class="container-fluid">
        <a class="navbar-brand" href="/" title="Apps">
          <img
            src="data:image/png;base64,
              
              
              
              "
            width="30"
            height="30"
          />
        </a>
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item dropdown">
            <h5>
              <a
                class="nav-link breadcrumb-item active"
                aria-current="page"
                href="/warehouse"
                role="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
                style="color: #fff !important"
              >
                Virtual Ware House Operation
              </a>
            </h5>
          </li>
        </ul>
        <form class="d-flex" role="search">
          <input
            class="form-control me-2"
            type="search"
            placeholder="Search"
            aria-label="Search"
          />
          <button class="btn btn-success" type="submit">Search</button>
        </form>
      </div>
    </nav>

    <div class="container p-2">
      <%if(user.role ==="Admin" ){%>
        <button class="btn btn-success m-3" onclick="document.getElementById('myModal').style.display='block'">Create W-House</button>
      <%}%>

      <!-- container for all ware housese Accordion -->
      <%if(wareHouse.length > 0){%>
      <!-- the accordion  -->
      <div class="container overflow-hidden flex-wrap d-flex">
        <!--FOR VIRTUAL WARE HOUSE  -->

        <div class="card m-1 mb-3 shadow-sm shadow-sm" style="width:13rem;" title="VIRTUAL WARE HOUSE" >
          <div class="card-img-top justify-content-center d-flex p-2">
              <img
              src="data:image/png;base64," 
              width=50 
              height="50">
          </div>
              <div  class="card-body">
                  <h5  class="card-title">VIRTUAL WARE HOUSE</h5>
                  <a href="" data-doc=""  class="btn btn-warning"> 
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-gear" viewBox="0 0 16 16">
                      <path d="M8 4.754a3.246 3.246 0 1 0 0 6.492 3.246 3.246 0 0 0 0-6.492zM5.754 8a2.246 2.246 0 1 1 4.492 0 2.246 2.246 0 0 1-4.492 0z"/>
                      <path d="M9.796 1.343c-.527-1.79-3.065-1.79-3.592 0l-.094.319a.873.873 0 0 1-1.255.52l-.292-.16c-1.64-.892-3.433.902-2.54 2.541l.159.292a.873.873 0 0 1-.52 1.255l-.319.094c-1.79.527-1.79 3.065 0 3.592l.319.094a.873.873 0 0 1 .52 1.255l-.16.292c-.892 1.64.901 3.434 2.541 2.54l.292-.159a.873.873 0 0 1 1.255.52l.094.319c.527 1.79 3.065 1.79 3.592 0l.094-.319a.873.873 0 0 1 1.255-.52l.292.16c1.64.893 3.434-.902 2.54-2.541l-.159-.292a.873.873 0 0 1 .52-1.255l.319-.094c1.79-.527 1.79-3.065 0-3.592l-.319-.094a.873.873 0 0 1-.52-1.255l.16-.292c.893-1.64-.902-3.433-2.541-2.54l-.292.159a.873.873 0 0 1-1.255-.52l-.094-.319zm-2.633.283c.246-.835 1.428-.835 1.674 0l.094.319a1.873 1.873 0 0 0 2.693 1.115l.291-.16c.764-.415 1.6.42 1.184 1.185l-.159.292a1.873 1.873 0 0 0 1.116 2.692l.318.094c.835.246.835 1.428 0 1.674l-.319.094a1.873 1.873 0 0 0-1.115 2.693l.16.291c.415.764-.42 1.6-1.185 1.184l-.291-.159a1.873 1.873 0 0 0-2.693 1.116l-.094.318c-.246.835-1.428.835-1.674 0l-.094-.319a1.873 1.873 0 0 0-2.692-1.115l-.292.16c-.764.415-1.6-.42-1.184-1.185l.159-.291A1.873 1.873 0 0 0 1.945 8.93l-.319-.094c-.835-.246-.835-1.428 0-1.674l.319-.094A1.873 1.873 0 0 0 3.06 4.377l-.16-.292c-.415-.764.42-1.6 1.185-1.184l.292.159a1.873 1.873 0 0 0 2.692-1.115l.094-.319z"/>
                    </svg>
                  </a>
              </div>
      </div> 

        <!-- ENDS HERE -->
        <%wareHouse.forEach((WH)=>{%>
          <div class="card m-1 mb-3 shadow-sm shadow-sm" style="width:13rem;" title="<%=WH.WHName%>" >
            <div class="card-img-top justify-content-center d-flex p-2">
                <img src="data:image/png;base64,"
                width=50 
                height="50">
            </div>
                <div  class="card-body">
                    <h5  class="card-title"><%= WH.WHName.toUpperCase() %></h5>
                    <p>Status: <%if(WH.Status ){%>
                        Activated
                    <%}else{%>
                        Deactivated
                    <%}%></p>
                    <a href="/Location/<%=WH._id%>" data-doc="<%=WH._id%>"  class="btn btn-primary"> 
                      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-gear" viewBox="0 0 16 16">
                        <path d="M8 4.754a3.246 3.246 0 1 0 0 6.492 3.246 3.246 0 0 0 0-6.492zM5.754 8a2.246 2.246 0 1 1 4.492 0 2.246 2.246 0 0 1-4.492 0z"/>
                        <path d="M9.796 1.343c-.527-1.79-3.065-1.79-3.592 0l-.094.319a.873.873 0 0 1-1.255.52l-.292-.16c-1.64-.892-3.433.902-2.54 2.541l.159.292a.873.873 0 0 1-.52 1.255l-.319.094c-1.79.527-1.79 3.065 0 3.592l.319.094a.873.873 0 0 1 .52 1.255l-.16.292c-.892 1.64.901 3.434 2.541 2.54l.292-.159a.873.873 0 0 1 1.255.52l.094.319c.527 1.79 3.065 1.79 3.592 0l.094-.319a.873.873 0 0 1 1.255-.52l.292.16c1.64.893 3.434-.902 2.54-2.541l-.159-.292a.873.873 0 0 1 .52-1.255l.319-.094c1.79-.527 1.79-3.065 0-3.592l-.319-.094a.873.873 0 0 1-.52-1.255l.16-.292c.893-1.64-.902-3.433-2.541-2.54l-.292.159a.873.873 0 0 1-1.255-.52l-.094-.319zm-2.633.283c.246-.835 1.428-.835 1.674 0l.094.319a1.873 1.873 0 0 0 2.693 1.115l.291-.16c.764-.415 1.6.42 1.184 1.185l-.159.292a1.873 1.873 0 0 0 1.116 2.692l.318.094c.835.246.835 1.428 0 1.674l-.319.094a1.873 1.873 0 0 0-1.115 2.693l.16.291c.415.764-.42 1.6-1.185 1.184l-.291-.159a1.873 1.873 0 0 0-2.693 1.116l-.094.318c-.246.835-1.428.835-1.674 0l-.094-.319a1.873 1.873 0 0 0-2.692-1.115l-.292.16c-.764.415-1.6-.42-1.184-1.185l.159-.291A1.873 1.873 0 0 0 1.945 8.93l-.319-.094c-.835-.246-.835-1.428 0-1.674l.319-.094A1.873 1.873 0 0 0 3.06 4.377l-.16-.292c-.415-.764.42-1.6 1.185-1.184l.292.159a1.873 1.873 0 0 0 2.692-1.115l.094-.319z"/>
                      </svg>
                    </a>
                </div>
        </div> 
        <%})%>
      </div>

      <%}%>
    <!-- The Modal -->
    <div
      id="myModal"
      class="modal flex items-center flex-col justify-center gap-3"
    >
      <!-- Modal content -->
      <div class="modal-content container p-3">
        <div class="d-flex  justify-content-between container">
          <h5 id="myModalText d-flex align-self-start" class="fw-bolder">New Entry</h5>
          <svg xmlns="http://www.w3.org/2000/svg" title="close Modal" onclick="document.getElementById('myModal').style.display='none'" width="16" height="16" fill="currentColor" class="bi bi-x-lg  border" viewBox="0 0 16 16">
            <path d="M2.146 2.854a.5.5 0 1 1 .708-.708L8 7.293l5.146-5.147a.5.5 0 0 1 .708.708L8.707 8l5.147 5.146a.5.5 0 0 1-.708.708L8 8.707l-5.146 5.147a.5.5 0 0 1-.708-.708L7.293 8 2.146 2.854Z"/>
          </svg>
        </div>
        <form action="" id="inventoryForm" method="post">
        <label for="productList" class="form-label fw-bolder"
          >Choose From Product List</label
        >
        <input
          class="form-control"
          list="datalistOptions"
          id="productList"
          name="productList"
          placeholder="Type to search product list..."
        />
        <datalist id="datalistOptions">
          <!-- display options for product -->
          <%if(Products){%> <%Products.forEach((product)=>{%>
          <option value="<%=product._id%>">
            <%=product.Name%> (<%=product.UMO%>)
          </option>
          <%})%> <%}%>
        </datalist>

        <label class="flex flex-row gap-9 justify-around m-2 fw-bolder"
          >Select Location</label
        >
        <!-- display options for product -->
        <%if(wareHouse){%> <%wareHouse.forEach((wareHouse)=>{%>
        <div class="input-group">
          <div class="input-group-text">
            <input
              class="form-check-input mt-0"
              type="radio"
              aria-label="Radio button for following text input"
              id="wareHouse"
              name="wareHouse"
              data-id="<%=wareHouse._id%>"
            />
          </div>
          <input
            type="text"
            class="form-control"
            aria-label="Text input with radio button"
            value="<%=wareHouse.WHName%>"
            disabled
          />
          <%})%> <%}%>
        </div>
      </div>
      <button type="submit" class="btn btn-success">Register Product</button>
    </form>
    </div>
  </body>
  <script>
    //get value for selected ware House
    const inventoryForm = document.getElementById('inventoryForm')

    inventoryForm.addEventListener('submit',async function (e){
      e.preventDefault()
      const wareHouseId = document.querySelectorAll('#wareHouse');
      let  wareHouse 

      for(let i=0;i < wareHouseId.length; i++) {
          if (wareHouseId[i].checked){
            wareHouse =  wareHouseId[i].dataset.id;
          }
          
        }

      const product = inventoryForm.elements.productList.value;
      const endPoint = `/wareHouseToTransfer`

      const request = await fetch(endPoint,{
        method:'POST',
        body: JSON.stringify({WHIDS:wareHouse,productId:product}),
        headers: {"Content-Type": 'application/json'}
      })

      const response = await request.json()
      response?.message? alert(response.message) : alert(response.Errormessage)
      response?.message? inventoryForm.reset() : ""
      location.reload();
    })
    
    // deactivate ware house
      const wareHouseId = document.querySelectorAll('#toggleReplenish');
      for(let i=0;i < wareHouseId.length; i++) {
        wareHouseId[i].addEventListener('change', async function(e){
          wareHouse =  wareHouseId[i].dataset.toggle;
          const update = {Status:wareHouseId[i].checked};
          const request = await fetch(`/warehouse/${wareHouse}/edit`,{
            body:JSON.stringify(update),
            method: 'PATCH',
            headers: {'Content-Type': 'application/json'},
          })
          const response = await request.json().then((res)=>{
            alert(res.result || res.error)
          });
        })
          
          
        }
  </script>
</html>
