<!DOCTYPE html>
<html lang="en">
  <%-include('./partials/head.ejs')%>
  <body>
    <nav class="navbar bg-black">
      <div class="container-fluid">
        <span class="navbar-brand" onclick="history.back()" href="/" title="Back">
          <img 
          width="30"
          hegith="30"
          src="data:image/png;base64,">
         </span>
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item dropdown">
            <h5>
              <a
                class="nav-link breadcrumb-item active"
                aria-current="page"
                href="/"
                role="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
                style="color: #fff !important"
              >
               Vendors Bills
              </a>
            </h5>
          </li>
        </ul>
        <form class="d-flex" role="search">
          <input
            class="form-control me-2"
            type="search"
            placeholder="Search"
            aria-label="Search"
          />
          <button class="btn btn-success" type="submit">Search</button>
        </form>
      </div>
    </nav>
     <!--bread crum div -->
     <div class="d-flex justify-content-between p-2 m-1 bg-success-subtle ">
        <h5 class="font-bold"></h5>
        <%if(user.role === "Admin"){%>
            <span onclick="location.assign('')" title="Download in Xlsx">
                <img height="40" width="40" src="data:image/png;base64,">
              </span>
        <%}%> 
       </div>
    <div class="h-59 p-1 d-flex flex-column" >
        <%if(VendorBills.length > 0){%>
            <%VendorBills.forEach(VendorBill=>{%>
                <span  class="list-group position-relative mb-2" title="Expense for <%=VendorBill.Amount%>">
                    <div class="list-group-item list-group-item-action d-flex justify-content-between align-items-center" >
                        <small class="position-absolute top-0  start-50 translate-middle badge rounded-pill bg-warning"title='grand total of this bill'>
                          NGN<%=VendorBill.Amount%>
                           <%if(VendorBill.status==='Approved'){%>
                          <span class="badge bg-secondary">EXPENSED</span>
                          <%}%>
                        </small>

                        <div class="btn-group">
                          <div class="btn-group dropstart" style="border-right: 1px dotted rgb(190, 190, 190) !important;" role="group">
                          <button type="button" class="btn btn-secondary dropdown-toggle dropdown-toggle-split" data-bs-toggle="MODAL" aria-expanded="false">
                            <span class="visually-hidden">Toggle Dropstart</span>
                            <img src="data:image/png;base64,"
                            width="20"height="20" >
                          </button>
                          </div>
                        </div>
                           
                          <div>
                            <% vend = vendor.filter(vendor=>{%>
                              <%return vendor._id.toString() === `${VendorBill.Vendor}`  %>
                              <%})%>
                              <%vend.map(name=>{%>
                               <h6>
                                <a class="link-offset-2 link-offset-3-hover link-underline link-underline-opacity-0 link-underline-opacity-75-hover" href="/Purchase/bill/<%=VendorBill.billReferenceNo%>">GNO/<%=VendorBill.billReferenceNo%>/<%=name.Name.substring(0,3).toUpperCase()%></a>
                               </h6>
                              <%})%>
                          </div>
                          
                          <div>
                            <p>Raised Date: <%=VendorBill.createdAt.toLocaleString()%></p>
                          </div>
                            <p title="reference Number" >NGN <%=VendorBill.Amount%></p>
                      </div> 
                    </span>
            <%})%>
        <%}else{%>
        <div class="container">
            <p>Sadly,You havent bought a thing yet..!!</p>
        </div>
        <%}%>
    </div>

     <!-- The Modal -->
     <div id="myModal" class="modal flex items-center flex-col justify-center gap-3 shadow">
        <!-- Modal content -->
        <div class="modal-body modal-content container">
            
        </div>
     </div>

    </body>
</html>